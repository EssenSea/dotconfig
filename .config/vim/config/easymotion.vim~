" (easymotion-prefix)f{char} to move to {char}
map  (easymotion-prefix)f <Plug>(easymotion-bd-f)
nmap (easymotion-prefix)f <Plug>(easymotion-overwin-f)

" s{char}{char} to move to {char}{char}
nmap s <Plug>(easymotion-overwin-f2)

" Move to line
map (easymotion-prefix)L <Plug>(easymotion-bd-jk)
nmap (easymotion-prefix)L <Plug>(easymotion-overwin-line)

" Move to word
map  (easymotion-prefix)w <Plug>(easymotion-bd-w)
nmap (easymotion-prefix)w <Plug>(easymotion-overwin-w)
